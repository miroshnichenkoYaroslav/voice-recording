{
  "manifest_version": 2,
  "name": "Audio recording extension",
  "version": "1.0",
  "description": "Audio recording extension desc",
  "permissions": [
    "activeTab",
    "storage",
    "webNavigation",
    "https://*/*",
    "devtools",
    "audioCapture"
  ],
  "browser_action": {
    //    "default_icon": {
    //      "16": "data:image/png;base64,"
    //    },
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
